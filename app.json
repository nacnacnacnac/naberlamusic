{
  "expo": {
    "name": "naber-la",
    "slug": "naber-la",
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-1024x1024.png",
    "scheme": "naberla",
    "linking": {
      "prefixes": ["naberla://"],
      "config": {
        "screens": {
          "oauth-callback": "oauth/callback"
        }
      }
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "120": "./assets/images/icon-120x120.png",
        "152": "./assets/images/icon-152x152.png",
        "167": "./assets/images/icon-167x167.png",
        "180": "./assets/images/icon-180x180.png",
        "20": "./assets/images/icon-20x20.png",
        "29": "./assets/images/icon-29x29.png",
        "40": "./assets/images/icon-40x40.png",
        "58": "./assets/images/icon-58x58.png",
        "76": "./assets/images/icon-76x76.png",
        "80": "./assets/images/icon-80x80.png",
        "87": "./assets/images/icon-87x87.png"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Bu uygulama video çekmek için kameraya erişim gerektirir.",
        "NSMicrophoneUsageDescription": "Bu uygulama ses kaydetmek için mikrofona erişim gerektirir.",
        "NSPhotoLibraryUsageDescription": "Bu uygulama fotoğraf ve videolara erişim gerektirir.",
        "UIBackgroundModes": [
          "audio"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "UIStatusBarHidden": false,
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIHomeIndicatorAutoHidden": true,
        "UIRequiresFullScreen": true,
        "UIPrefersHomeIndicatorAutoHidden": true,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "UILaunchStoryboardName": "SplashScreen"
      },
      "bundleIdentifier": "com.naber.la"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK",
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK"
      ],
      "package": "com.naber.la"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./public/favicon.ico"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Bu uygulama fotoğraf ve videolara erişim gerektirir.",
          "savePhotosPermission": "Bu uygulama fotoğraf ve video kaydetmek için erişim gerektirir.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.852958067531-6b47ucgkstt9g1g2ok71corb7qrmhics"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8316266b-416f-49cc-b9a5-56082e2914d8"
      },
      "EXPO_PUBLIC_VIMEO_ACCESS_TOKEN": "f0e1fe38a2b99765df33777ff57e3aaa",
      "EXPO_PUBLIC_USE_BACKEND_TOKENS": "true",
      "EXPO_PUBLIC_BACKEND_URL": "https://api.naberla.org",
      "EXPO_PUBLIC_APP_ID": "naber-la-mobile"
    },
    "owner": "nacnac"
  }
}
