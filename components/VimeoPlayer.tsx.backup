import React, { useState, useRef, useEffect, useMemo } from 'react';
import { StyleSheet, View, ActivityIndicator, TouchableOpacity, Alert, Image } from 'react-native';
import { WebView } from 'react-native-webview';
import AsyncStorage from '@react-native-async-storage/async-storage';
import { ThemedText } from '@/components/ThemedText';
import { ThemedView } from '@/components/ThemedView';
import { IconSymbol } from '@/components/ui/IconSymbol';
import { SimplifiedVimeoVideo } from '@/types/vimeo';

interface VimeoPlayerProps {
  video: SimplifiedVimeoVideo;
  isFullscreen?: boolean;
  onFullscreenToggle?: () => void;
  onError?: (error: string) => void;
  onVideoEnd?: () => void;
  isPaused?: boolean;
  onPlayStateChange?: (isPlaying: boolean) => void;
  onTimeUpdate?: (currentTime: number, duration: number) => void;
}

export default function VimeoPlayer({ 
  video, 
  isFullscreen = false, 
  onFullscreenToggle,
  onError,
  onVideoEnd,
  isPaused = false,
  onPlayStateChange,
  onTimeUpdate
}: VimeoPlayerProps) {
  const [isLoading, setIsLoading] = useState(true);
  const [hasError, setHasError] = useState(false);
  const webViewRef = useRef<WebView>(null);
  const [internalPaused, setInternalPaused] = useState(isPaused);
  const [savedPosition, setSavedPosition] = useState<number>(0);
  const [isPlayerReady, setIsPlayerReady] = useState(false);

  // Video deÄŸiÅŸtiÄŸinde kaydedilmiÅŸ pozisyonu yÃ¼kle
  useEffect(() => {
    loadSavedPosition();
    setIsPlayerReady(false);
  }, [video.id]);

  // isPaused prop deÄŸiÅŸikliklerini takip et - GÃœÃ‡LÃœ AUTOPLAY
  useEffect(() => {
    console.log('ðŸ”„ VimeoPlayer useEffect - isPaused:', isPaused, 'internal:', internalPaused, 'playerReady:', isPlayerReady);
    
    if (isPaused !== internalPaused) {
      setInternalPaused(isPaused);
      
      const command = isPaused ? 'pause' : 'play';
      console.log('ðŸ“¡ SENDING COMMAND TO WEBVIEW:', command);
      
      // Her durumda komut gÃ¶nder (player ready olsun olmasÄ±n)
      webViewRef.current?.postMessage(JSON.stringify({ action: command }));
      
      if (!isPlayerReady) {
        console.log('â³ Player not ready, but sending command anyway');
        // Player hazÄ±r deÄŸilse birkaÃ§ kez daha dene
        setTimeout(() => {
          console.log('ðŸ”„ Retry command (1s delay):', command);
          webViewRef.current?.postMessage(JSON.stringify({ action: command }));
        }, 1000);
        
        setTimeout(() => {
          console.log('ðŸ”„ Retry command (2s delay):', command);
          webViewRef.current?.postMessage(JSON.stringify({ action: command }));
        }, 2000);
      }
    } else {
      console.log('ðŸ”„ No change in pause state, skipping command');
    }
  }, [isPaused, internalPaused, isPlayerReady]);

  // KaydedilmiÅŸ pozisyonu yÃ¼kle
  const loadSavedPosition = async () => {
    try {
      const saved = await AsyncStorage.getItem(`video_position_${video.id}`);
      if (saved) {
        const position = parseFloat(saved);
        setSavedPosition(position);
        console.log('Loaded saved position for video', video.id, ':', position);
      } else {
        setSavedPosition(0);
      }
    } catch (error) {
      console.error('Error loading saved position:', error);
      setSavedPosition(0);
    }
  };

  // Video pozisyonunu kaydet
  const savePosition = async (position: number) => {
    try {
      await AsyncStorage.setItem(`video_position_${video.id}`, position.toString());
      console.log('Saved position for video', video.id, ':', position);
    } catch (error) {
      console.error('Error saving position:', error);
    }
  };

  // Vimeo embed URL oluÅŸtur - MEMOIZED (sadece video.id deÄŸiÅŸince yeniden oluÅŸtur)
  const embedUrl = useMemo(() => {
    // Video ID'yi temizle (sadece sayÄ±sal kÄ±sÄ±m)
    let cleanVideoId = video.id.replace(/\D/g, '');
    
    // EÄŸer video ID boÅŸsa veya Ã§ok kÄ±saysa, test video kullan
    if (!cleanVideoId || cleanVideoId.length < 6) {
      console.warn('Invalid video ID, using test video');
      cleanVideoId = '76979871'; // Vimeo test video
    }
    
    const baseUrl = `https://player.vimeo.com/video/${cleanVideoId}`;
    const params = new URLSearchParams({
      autoplay: '1', // Her zaman autoplay=1 - JavaScript ile kontrol edeceÄŸiz
      loop: '0',
      muted: '0',
      controls: '0', // Native controls'Ã¼ kapat - sadece Media Session kullan
      title: '0',
      byline: '0',
      portrait: '0',
      badge: '0',
      autopause: '0', // Autopause'u da kapat
      background: '0',
      responsive: '1',
      dnt: '1',
      pip: '0', // Picture-in-picture'Ä± kapat
      playsinline: '1',
      keyboard: '0',
      maxwidth: '100%',
      maxheight: '100%',
      transparent: '0',
      app_id: 'naberla'
    });
    
    // KaydedilmiÅŸ pozisyon varsa URL'e ekle
    if (savedPosition > 0) {
      params.append('t', `${Math.floor(savedPosition)}s`);
      console.log('Adding saved position to URL:', savedPosition);
    }
    
    const finalUrl = `${baseUrl}?${params.toString()}`;
    console.log('Vimeo Embed URL:', finalUrl);
    console.log('Original Video ID:', video.id);
    console.log('Clean Video ID:', cleanVideoId);
    console.log('Video Title:', video.title);
    
    return finalUrl;
  }, [video.id, savedPosition]); // Sadece video.id veya savedPosition deÄŸiÅŸince yeniden oluÅŸtur

  // WebView mesajlarÄ±nÄ± handle et
  const handleMessage = (event: any) => {
    try {
      const data = JSON.parse(event.nativeEvent.data);
      console.log('Vimeo Player Message:', data);
      
      // Player events'leri handle edebiliriz
      switch (data.event) {
        case 'ready':
          console.log('ðŸŽ¯ VIMEO PLAYER READY!');
          setIsPlayerReady(true);
          
          // Player hazÄ±r olduÄŸunda kaydedilmiÅŸ pozisyona git
          if (savedPosition > 0) {
            setTimeout(() => {
              webViewRef.current?.postMessage(JSON.stringify({ 
                action: 'seekTo', 
                time: savedPosition 
              }));
              console.log('ðŸ“ Seeking to saved position:', savedPosition);
            }, 500);
          }
          
          // AUTOPLAY LOGIC - Player hazÄ±r olduÄŸunda isPaused durumuna bak
          setTimeout(() => {
            if (!isPaused) {
              console.log('ðŸš€ AUTOPLAY: Video should be playing (autoplay=1)');
              // autoplay=1 olduÄŸu iÃ§in zaten Ã§alÄ±yor olmalÄ±, ama emin olmak iÃ§in
              webViewRef.current?.postMessage(JSON.stringify({ action: 'play' }));
            } else {
              console.log('â¸ï¸ Video should be paused, sending pause command');
              webViewRef.current?.postMessage(JSON.stringify({ action: 'pause' }));
            }
          }, savedPosition > 0 ? 1500 : 800); // Pozisyon varsa biraz daha bekle
          break;
        case 'play':
          console.log('Video started playing');
          setInternalPaused(false);
          onPlayStateChange?.(true);
          // Media Session state'i gÃ¼ncelle
          webViewRef.current?.postMessage(JSON.stringify({ 
            action: 'updateMediaSession', 
            state: 'playing' 
          }));
          break;
        case 'pause':
          console.log('Video paused');
          setInternalPaused(true);
          onPlayStateChange?.(false);
          // Media Session state'i gÃ¼ncelle
          webViewRef.current?.postMessage(JSON.stringify({ 
            action: 'updateMediaSession', 
            state: 'paused' 
          }));
          break;
        case 'ended':
          console.log('Video ended');
          // Video bittiÄŸinde pozisyonu sÄ±fÄ±rla
          savePosition(0);
          if (onVideoEnd) {
            onVideoEnd();
          }
          break;
        case 'timeupdate':
          if (data.seconds && data.duration) {
            const currentTime = parseFloat(data.seconds);
            const duration = parseFloat(data.duration);
            // Her 5 saniyede bir pozisyonu kaydet
            if (Math.floor(currentTime) % 5 === 0) {
              savePosition(currentTime);
            }
            onTimeUpdate?.(currentTime, duration);
          }
          break;
        case 'background_mode':
          console.log('ðŸŒ™ Background mode detected from WebView');
          // Background mode'da ekstra bir ÅŸey yapmaya gerek yok, WebView halledecek
          break;
        case 'media_session_play':
          console.log('ðŸŽµ Media session play from lock screen');
          setInternalPaused(false);
          onPlayStateChange?.(true);
          break;
        case 'media_session_pause':
          console.log('â¸ï¸ Media session pause from lock screen');
          setInternalPaused(true);
          onPlayStateChange?.(false);
          break;
        case 'media_session_next':
          console.log('â­ï¸ Media session next track');
          onVideoEnd?.(); // Next track trigger
          break;
        case 'media_session_previous':
          console.log('â®ï¸ Media session previous track');
          // Previous track logic burada implement edilebilir
          break;
        case 'webview_test_response':
          console.log('ðŸ§ª WebView test response:', data.message);
          console.log('ðŸ” WebView has', data.iframes, 'iframes and', data.videos, 'videos');
          break;
      }
    } catch (error) {
      console.log('Message parsing error:', error);
    }
  };

  // Loading baÅŸladÄ±
  const handleLoadStart = () => {
    console.log('WebView loading started');
    setIsLoading(true);
    setHasError(false);
  };

  // Loading tamamlandÄ±
  const handleLoadEnd = () => {
    console.log('ðŸŒ WebView loading ended - injecting test message');
    setIsLoading(false);
    setHasError(false);
    
    // WebView yÃ¼klendiÄŸinde test mesajÄ± gÃ¶nder
    setTimeout(() => {
      console.log('ðŸ§ª Sending test message to WebView');
      webViewRef.current?.postMessage(JSON.stringify({ action: 'test', message: 'WebView connection test' }));
    }, 1000);
  };

  // Hata durumu
  const handleError = (syntheticEvent: any) => {
    const { nativeEvent } = syntheticEvent;
    console.error('WebView Error:', nativeEvent);
    console.error('Error description:', nativeEvent.description);
    console.error('Error code:', nativeEvent.code);
    setIsLoading(false);
    setHasError(true);
    onError?.(`Video yÃ¼klenemedi: ${nativeEvent.description || 'Bilinmeyen hata'}`);
  };

  // HTTP error handling
  const handleHttpError = (syntheticEvent: any) => {
    const { nativeEvent } = syntheticEvent;
    console.error('WebView HTTP Error:', nativeEvent);
    console.error('Status code:', nativeEvent.statusCode);
    console.error('URL:', nativeEvent.url);
    
    if (nativeEvent.statusCode >= 400) {
      setIsLoading(false);
      setHasError(true);
      const errorMsg = nativeEvent.statusCode === 404 
        ? `Video bulunamadÄ± veya embed edilmeye kapalÄ± (ID: ${video.id})`
        : `HTTP Error ${nativeEvent.statusCode}: Video yÃ¼klenemedi`;
      onError?.(errorMsg);
    }
  };

  // Retry fonksiyonu
  const retry = () => {
    setHasError(false);
    setIsLoading(true);
    webViewRef.current?.reload();
  };

  // Hata durumu UI
  if (hasError) {
    return (
      <ThemedView style={[styles.container, isFullscreen && styles.fullscreenContainer]}>
        <ThemedView style={styles.errorContainer}>
          <IconSymbol name="exclamationmark.triangle" size={48} color="#FF6B6B" />
          <ThemedText style={styles.errorTitle}>Video YÃ¼klenemedi</ThemedText>
          <ThemedText style={styles.errorText}>
            {video.title}
          </ThemedText>
          <TouchableOpacity style={styles.retryButton} onPress={retry}>
            <IconSymbol name="arrow.clockwise" size={20} color="white" />
            <ThemedText style={styles.retryText}>Tekrar Dene</ThemedText>
          </TouchableOpacity>
        </ThemedView>
        
        {/* Fullscreen Toggle */}
        {onFullscreenToggle && (
          <TouchableOpacity 
            style={styles.fullscreenButton}
            onPress={onFullscreenToggle}
          >
            <IconSymbol 
              name={isFullscreen ? "arrow.down.right.and.arrow.up.left" : "arrow.up.left.and.arrow.down.right"} 
              size={24} 
              color="white" 
            />
          </TouchableOpacity>
        )}
      </ThemedView>
    );
  }

  return (
    <ThemedView style={[styles.container, isFullscreen && styles.fullscreenContainer]}>
      {/* Loading Indicator */}
      {isLoading && (
        <ThemedView style={styles.loadingContainer}>
          <Image 
            source={require('@/assets/images/loading.gif')} 
            style={styles.loadingGif}
            resizeMode="contain"
          />
        </ThemedView>
      )}

      {/* Vimeo WebView */}
      <WebView
        ref={webViewRef}
        source={{ uri: embedUrl }}
        style={styles.webview}
        onLoadStart={handleLoadStart}
        onLoadEnd={handleLoadEnd}
        onError={handleError}
        onHttpError={handleHttpError}
        onMessage={handleMessage}
        allowsInlineMediaPlayback={true}
        mediaPlaybackRequiresUserAction={false}
        allowsFullscreenVideo={false}
        javaScriptEnabled={true}
        domStorageEnabled={true}
        startInLoadingState={false}
        scalesPageToFit={true}
        bounces={false}
        scrollEnabled={false}
        showsHorizontalScrollIndicator={false}
        showsVerticalScrollIndicator={false}
        // GÃœÃ‡LÃœ BACKGROUND AUDIO AYARLARI
        allowsBackForwardNavigationGestures={false}
        allowsLinkPreview={false}
        mixedContentMode="compatibility"
        thirdPartyCookiesEnabled={true}
        sharedCookiesEnabled={true}
        allowsAirPlayForMediaPlayback={true}
        allowsProtectedMedia={true}
        setSupportMultipleWindows={false}
        nestedScrollEnabled={false}
        overScrollMode="never"
        cacheEnabled={true}
        incognito={false}
        applicationNameForUserAgent="NaberLA-Music-Player"
        // Background audio iÃ§in kritik iOS ayarlarÄ±
        allowsBackgroundLocationUpdates={false}
        automaticallyAdjustContentInsets={false}
        // Background playback iÃ§in ek ayarlar
        pullToRefreshEnabled={false}
        hideKeyboardAccessoryView={true}
        // User agent (bazÄ± durumlarda gerekli olabilir)
        userAgent="Mozilla/5.0 (iPhone; CPU iPhone OS 14_0 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/14.0 Mobile/15E148 Safari/604.1"
        // BASIT VE KISA INJECTED JAVASCRIPT
        injectedJavaScript={`
          console.log('ðŸš€ WEBVIEW JAVASCRIPT STARTED');
          
          // React Native'den gelen mesajlarÄ± handle et
          window.addEventListener('message', function(event) {
            if (event.source === window) {
              try {
                const data = JSON.parse(event.data);
                console.log('ðŸ“¨ Message received:', data.action);
                
                if (data.action === 'test') {
                  console.log('ðŸ§ª TEST MESSAGE RECEIVED');
                  window.ReactNativeWebView.postMessage(JSON.stringify({
                    event: 'webview_test_response',
                    message: 'JavaScript is working!'
                  }));
                  return;
                }
                
                const iframe = document.querySelector('iframe');
                
                if (data.action === 'play') {
                  console.log('ðŸŽ¬ PLAY COMMAND RECEIVED');
                  if (iframe && iframe.contentWindow) {
                    iframe.contentWindow.postMessage('{"method":"play"}', '*');
                    console.log('ðŸ“¡ Play sent to iframe');
                  } else {
                    console.log('âŒ No iframe found');
                  }
                } else if (data.action === 'pause') {
                  console.log('â¸ï¸ PAUSE COMMAND RECEIVED');
                  if (iframe && iframe.contentWindow) {
                    iframe.contentWindow.postMessage('{"method":"pause"}', '*');
                    console.log('ðŸ“¡ Pause sent to iframe');
                  } else {
                    console.log('âŒ No iframe found');
                  }
                }
              } catch (e) {
                console.log('âŒ Message parse error:', e);
              }
            }
          });
          
          // Vimeo mesajlarÄ±nÄ± dinle
          window.addEventListener('message', function(event) {
            if (event.origin === 'https://player.vimeo.com') {
              console.log('ðŸ“º Vimeo message:', event.data);
              if (window.ReactNativeWebView) {
                window.ReactNativeWebView.postMessage(JSON.stringify(event.data));
              }
            }
          });
          
          console.log('âœ… WEBVIEW JAVASCRIPT SETUP COMPLETE');
          true;
        `}
      />

      {/* Fullscreen toggle kaldÄ±rÄ±ldÄ± - pil kÄ±smÄ±yla Ã§akÄ±ÅŸÄ±yor */}
    </ThemedView>
  );
}

