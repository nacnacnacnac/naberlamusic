          \`;
          document.head.appendChild(style);
          
          console.log('WebView JavaScript loaded with enhanced controls');
          console.log('Current URL:', window.location.href);
          
          // Vimeo Player API iÃ§in global deÄŸiÅŸkenler
          let vimeoPlayer = null;
          let isPlayerReady = false;
          
          // Background audio iÃ§in kritik ayarlar
          try {
            // Audio context'i unlock et ve background'da aktif tut
            let audioContext;
            if (typeof AudioContext !== 'undefined' || typeof webkitAudioContext !== 'undefined') {
              const AudioContextClass = AudioContext || webkitAudioContext;
              audioContext = new AudioContextClass();
              
              // Touch event ile audio context'i baÅŸlat
              document.addEventListener('touchstart', function() {
                if (audioContext.state === 'suspended') {
                  audioContext.resume();
                  console.log('AudioContext resumed');
                }
              }, { once: true });
              
            // AGRESIF BACKGROUND AUDIO Ã‡Ã–ZÃœMÃœ
            document.addEventListener('visibilitychange', function() {
              const videos = document.querySelectorAll('video');
              const iframe = document.querySelector('iframe');
              
              if (document.hidden) {
                console.log('ðŸŒ™ APP WENT TO BACKGROUND - FORCING AUDIO TO CONTINUE');
                
                // Audio context'i zorla aktif tut
                if (audioContext && audioContext.state === 'suspended') {
                  audioContext.resume();
                  console.log('ðŸ”Š AudioContext resumed for background');
                }
                
                // Video'larÄ± agresif ÅŸekilde Ã§almaya devam ettir
                videos.forEach(function(video) {
                  if (video.dataset.userPaused !== 'true') {
                    console.log('ðŸŽµ Forcing video to play in background');
                    video.play().catch(function(err) {
                      console.log('Background play error:', err);
                    });
                    
                    // Video'nun background'da Ã§almasÄ± iÃ§in Ã¶zel ayarlar
                    video.setAttribute('playsinline', 'true');
                    video.setAttribute('webkit-playsinline', 'true');
                    video.muted = false; // Ses aÃ§Ä±k olsun
                  }
                });
                
                // Vimeo iframe'e agresif play komutlarÄ±
                if (iframe && iframe.contentWindow) {
                  console.log('ðŸ“¡ Sending aggressive play commands to Vimeo iframe');
                  iframe.contentWindow.postMessage('{"method":"play"}', '*');
                  
                  // BirkaÃ§ kez daha gÃ¶nder
                  setTimeout(function() {
                    iframe.contentWindow.postMessage('{"method":"play"}', '*');
                  }, 100);
                  setTimeout(function() {
                    iframe.contentWindow.postMessage('{"method":"play"}', '*');
                  }, 500);
                  setTimeout(function() {
                    iframe.contentWindow.postMessage('{"method":"play"}', '*');
                  }, 1000);
                }
                
                // React Native'e background durumunu bildir
                if (window.ReactNativeWebView) {
                  window.ReactNativeWebView.postMessage(JSON.stringify({
                    event: 'background_mode',
                    playing: true
                  }));
                }
                
              } else {
                console.log('â˜€ï¸ APP RETURNED TO FOREGROUND');
                
                // Foreground'a dÃ¶nerken durumu kontrol et ve devam ettir
                videos.forEach(function(video) {
                  if (video.dataset.userPaused !== 'true' && video.paused) {
                    console.log('ðŸ”„ Resuming video in foreground');
                    video.play().catch(function(err) {
                      console.log('Foreground resume error:', err);
                    });
                  }
                });
                
                // Vimeo iframe'e de resume komutu
                if (iframe && iframe.contentWindow) {
                  iframe.contentWindow.postMessage('{"method":"play"}', '*');
                }
              }
            });
            
            // Page focus/blur events
            window.addEventListener('blur', function() {
              console.log('Window blur - maintaining audio');
              if (audioContext && audioContext.state === 'suspended') {
                audioContext.resume();
              }
              // Background'a geÃ§erken video durumunu koru
              const videos = document.querySelectorAll('video');
              videos.forEach(function(video) {
                if (!video.paused && video.dataset.userPaused !== 'true') {
                  console.log('Window blur - video should continue playing');
                  video.dataset.shouldPlayInBackground = 'true';
                }
              });
            });
            
            window.addEventListener('focus', function() {
              console.log('Window focus');
              // Focus'a dÃ¶nerken video durumunu kontrol et
              const videos = document.querySelectorAll('video');
              videos.forEach(function(video) {
                if (video.dataset.shouldPlayInBackground === 'true' && video.paused && video.dataset.userPaused !== 'true') {
                  console.log('Window focus - resuming video that should be playing');
                  video.play().catch(function(err) {
                    console.log('Focus resume error:', err);
                  });
                }
              });
            });
            }
            
            // GÃœÃ‡LÃœ MEDIA SESSION API (kilit ekranÄ± kontrolleri iÃ§in)
            if ('mediaSession' in navigator) {
              console.log('ðŸŽ›ï¸ Setting up Media Session API');
              
              // Metadata'yÄ± dinamik olarak gÃ¼ncelle
              const updateMediaMetadata = function(title) {
                navigator.mediaSession.metadata = new MediaMetadata({
                  title: title || 'Naber LA Music',
                  artist: 'Naber LA',
                  album: 'Music Collection',
                  artwork: [
                    { src: 'https://via.placeholder.com/96x96', sizes: '96x96', type: 'image/png' },
                    { src: 'https://via.placeholder.com/128x128', sizes: '128x128', type: 'image/png' },
                    { src: 'https://via.placeholder.com/192x192', sizes: '192x192', type: 'image/png' },
                    { src: 'https://via.placeholder.com/256x256', sizes: '256x256', type: 'image/png' },
                    { src: 'https://via.placeholder.com/384x384', sizes: '384x384', type: 'image/png' },
                    { src: 'https://via.placeholder.com/512x512', sizes: '512x512', type: 'image/png' }
                  ]
                });
                console.log('ðŸ“± Media metadata updated:', title);
              };
              
              updateMediaMetadata('Naber LA Music');
              
              // PLAY ACTION - Kilit ekranÄ±ndan play
              navigator.mediaSession.setActionHandler('play', function() {
                console.log('ðŸŽµ MEDIA SESSION PLAY - from lock screen');
                const videos = document.querySelectorAll('video');
                const iframe = document.querySelector('iframe');
                
                // Vimeo iframe'e play komutu
                if (iframe && iframe.contentWindow) {
                  iframe.contentWindow.postMessage('{"method":"play"}', '*');
                  console.log('ðŸ“¡ Sent play to Vimeo iframe from media session');
                }
                
                // Video elementlerine de play
                if (videos.length > 0) {
                  videos[0].play().catch(function(err) {
                    console.log('Media session play error:', err);
                  });
                }
                
                // React Native'e bildir
                if (window.ReactNativeWebView) {
                  window.ReactNativeWebView.postMessage(JSON.stringify({
                    event: 'media_session_play'
                  }));
                }
              });
              
              // PAUSE ACTION - Kilit ekranÄ±ndan pause
              navigator.mediaSession.setActionHandler('pause', function() {
                console.log('â¸ï¸ MEDIA SESSION PAUSE - from lock screen');
                const videos = document.querySelectorAll('video');
                const iframe = document.querySelector('iframe');
                
                // Vimeo iframe'e pause komutu
                if (iframe && iframe.contentWindow) {
                  iframe.contentWindow.postMessage('{"method":"pause"}', '*');
                  console.log('ðŸ“¡ Sent pause to Vimeo iframe from media session');
                }
                
                // Video elementlerine de pause
                if (videos.length > 0) {
                  videos[0].pause();
                }
                
                // React Native'e bildir
                if (window.ReactNativeWebView) {
                  window.ReactNativeWebView.postMessage(JSON.stringify({
                    event: 'media_session_pause'
                  }));
                }
              });
              
              // NEXT/PREVIOUS TRACK (opsiyonel)
              navigator.mediaSession.setActionHandler('nexttrack', function() {
                console.log('â­ï¸ MEDIA SESSION NEXT TRACK');
                if (window.ReactNativeWebView) {
                  window.ReactNativeWebView.postMessage(JSON.stringify({
                    event: 'media_session_next'
                  }));
                }
              });
              
              navigator.mediaSession.setActionHandler('previoustrack', function() {
                console.log('â®ï¸ MEDIA SESSION PREVIOUS TRACK');
                if (window.ReactNativeWebView) {
                  window.ReactNativeWebView.postMessage(JSON.stringify({
                    event: 'media_session_previous'
                  }));
                }
              });
              
              // Playback state'i gÃ¼ncelle
              navigator.mediaSession.playbackState = 'playing';
              console.log('ðŸŽ›ï¸ Media Session setup complete');
            }
            
            // Video elementlerini background'da aktif tut - BASIT Ã‡Ã–ZÃœM
            const keepVideoActive = function() {
              const videos = document.querySelectorAll('video');
              videos.forEach(function(video) {
                // Background audio iÃ§in kritik ayarlar
                video.setAttribute('playsinline', 'true');
                video.setAttribute('webkit-playsinline', 'true');
                video.removeAttribute('controls'); // Native kontrolleri tamamen kaldÄ±r
                video.controls = false; // JavaScript ile de kapat
                
                // iOS native player'Ä±nÄ± devre dÄ±ÅŸÄ± bÄ±rak
                video.setAttribute('disablePictureInPicture', 'true');
                video.setAttribute('controlsList', 'nodownload nofullscreen noremoteplayback');
                
                // User pause durumunu track et
                video.addEventListener('pause', function(e) {
                  if (!document.hidden) {
                    video.dataset.userPaused = 'true';
                    console.log('User paused video');
                    if (window.ReactNativeWebView) {
                      window.ReactNativeWebView.postMessage(JSON.stringify({event: 'pause'}));
                    }
                  } else {
                    console.log('System paused video (background)');
                  }
                });
                
                // User play durumunu track et
                video.addEventListener('play', function(e) {
                  video.dataset.userPaused = 'false';
                  console.log('User played video');
                  if (window.ReactNativeWebView) {
                    window.ReactNativeWebView.postMessage(JSON.stringify({event: 'play'}));
                  }
                });
                
                // Video metadata'sÄ±nÄ± media session'a aktar
                video.addEventListener('loadedmetadata', function() {
                  if ('mediaSession' in navigator && navigator.mediaSession.metadata) {
                    navigator.mediaSession.metadata.title = document.title || 'Naber LA Music';
                  }
                });
              });
            };
            
            // DOM ready olduÄŸunda video'larÄ± kontrol et
            if (document.readyState === 'loading') {
              document.addEventListener('DOMContentLoaded', keepVideoActive);
            } else {
              keepVideoActive();
            }
            
            // Yeni video elementleri iÃ§in observer
            const observer = new MutationObserver(function(mutations) {
              mutations.forEach(function(mutation) {
                if (mutation.addedNodes) {
                  mutation.addedNodes.forEach(function(node) {
                    if (node.tagName === 'VIDEO') {
                      keepVideoActive();
                    }
                  });
                }
              });
            });
            
            observer.observe(document.body, {
              childList: true,
              subtree: true
            });
            
          } catch (error) {
            console.log('Background audio setup error:', error);
          }
          
          // React Native'den gelen mesajlarÄ± handle et
          if (window.ReactNativeWebView) {
            window.addEventListener('message', function(event) {
              if (event.source === window) {
                try {
                  const data = JSON.parse(event.data);
                  console.log('Received message from React Native:', data);
                  
                  if (data.action) {
                    const videos = document.querySelectorAll('video');
                    const iframe = document.querySelector('iframe');
                    
                    // Ã–NCE VIMEO IFRAME'E KOMUT GÃ–NDER (ANA PLAYER)
                    if (data.action === 'play') {
                      console.log('ðŸŽ¬ EXECUTING PLAY COMMAND - SUPER AGGRESSIVE MODE');
                      
                      // 1. Vimeo iframe'e play komutu gÃ¶nder (ana player)
                      if (iframe && iframe.contentWindow) {
                        console.log('ðŸ“¡ Sending play to Vimeo iframe');
                        iframe.contentWindow.postMessage('{"method":"play"}', '*');
                        
                        // Ã‡oklu retry
                        setTimeout(function() {
                          iframe.contentWindow.postMessage('{"method":"play"}', '*');
                          console.log('ðŸ”„ Retry 1: play command sent');
                        }, 100);
                        setTimeout(function() {
                          iframe.contentWindow.postMessage('{"method":"play"}', '*');
                          console.log('ðŸ”„ Retry 2: play command sent');
                        }, 300);
                        setTimeout(function() {
                          iframe.contentWindow.postMessage('{"method":"play"}', '*');
                          console.log('ðŸ”„ Retry 3: play command sent');
                        }, 600);
                      } else {
                        console.log('âŒ Vimeo iframe not found initially');
                      }
                      
                      // 2. iframe bulunamazsa sÃ¼rekli ara
                      let retryCount = 0;
                      const findIframeInterval = setInterval(function() {
                        const retryIframe = document.querySelector('iframe');
                        retryCount++;
                        
                        if (retryIframe && retryIframe.contentWindow) {
                          console.log('âœ… Found iframe after', retryCount, 'attempts, sending play');
                          retryIframe.contentWindow.postMessage('{"method":"play"}', '*');
                          clearInterval(findIframeInterval);
                        } else if (retryCount > 10) {
                          console.log('âŒ Could not find iframe after 10 attempts');
                          clearInterval(findIframeInterval);
                        }
                      }, 200);
                      
                      // Backup olarak video elementlerine de gÃ¶nder
                      if (videos.length > 0) {
                        console.log('ðŸ“º Sending play to video elements');
                        videos[0].play().catch(function(err) {
                          console.log('Video play error:', err);
                        });
                      }
                    } else if (data.action === 'pause') {
                      console.log('â¸ï¸ EXECUTING PAUSE COMMAND - SUPER AGGRESSIVE MODE');
                      
                      // 1. Vimeo iframe'e pause komutu gÃ¶nder (ana player)
                      if (iframe && iframe.contentWindow) {
                        console.log('ðŸ“¡ Sending pause to Vimeo iframe');
                        iframe.contentWindow.postMessage('{"method":"pause"}', '*');
                        
                        // Ã‡oklu retry
                        setTimeout(function() {
                          iframe.contentWindow.postMessage('{"method":"pause"}', '*');
                          console.log('ðŸ”„ Retry 1: pause command sent');
                        }, 100);
                        setTimeout(function() {
                          iframe.contentWindow.postMessage('{"method":"pause"}', '*');
                          console.log('ðŸ”„ Retry 2: pause command sent');
                        }, 300);
                      } else {
                        console.log('âŒ Vimeo iframe not found for pause');
                      }
                      
                      // 2. iframe bulunamazsa sÃ¼rekli ara
                      let retryCount = 0;
                      const findIframeInterval = setInterval(function() {
                        const retryIframe = document.querySelector('iframe');
                        retryCount++;
                        
                        if (retryIframe && retryIframe.contentWindow) {
                          console.log('âœ… Found iframe for pause after', retryCount, 'attempts');
                          retryIframe.contentWindow.postMessage('{"method":"pause"}', '*');
                          clearInterval(findIframeInterval);
                        } else if (retryCount > 5) {
                          console.log('âŒ Could not find iframe for pause after 5 attempts');
                          clearInterval(findIframeInterval);
                        }
                      }, 200);
                      
                      // Backup olarak video elementlerine de gÃ¶nder
                      if (videos.length > 0) {
                        console.log('ðŸ“º Sending pause to video elements');
                        videos[0].pause();
                      }
                    } else if (data.action === 'seekTo' && data.time) {
                      console.log('Executing seekTo command:', data.time);
                      // Vimeo iframe'e seekTo komutu gÃ¶nder (ana player)
                      if (iframe && iframe.contentWindow) {
                        iframe.contentWindow.postMessage(JSON.stringify({
                          method: 'setCurrentTime',
                          value: data.time
                        }), '*');
                      }
                      // Backup olarak video elementlerine de gÃ¶nder
                      if (videos.length > 0) {
                        videos[0].currentTime = data.time;
                      }
                    } else if (data.action === 'updateMediaSession' && data.state) {
                      console.log('ðŸŽ›ï¸ Updating media session state:', data.state);
                      if ('mediaSession' in navigator) {
                        navigator.mediaSession.playbackState = data.state;
                        console.log('ðŸ“± Media session state updated to:', data.state);
                      }
                    } else if (data.action === 'test') {
                      console.log('ðŸ§ª TEST MESSAGE RECEIVED IN WEBVIEW:', data.message);
                      console.log('ðŸ” Current iframe count:', document.querySelectorAll('iframe').length);
                      console.log('ðŸ” Current video count:', document.querySelectorAll('video').length);
                      
                      // React Native'e test cevabÄ± gÃ¶nder
                      if (window.ReactNativeWebView) {
                        window.ReactNativeWebView.postMessage(JSON.stringify({
                          event: 'webview_test_response',
                          message: 'WebView JavaScript is working!',
                          iframes: document.querySelectorAll('iframe').length,
                          videos: document.querySelectorAll('video').length
                        }));
                      }
                    }
                  }
                } catch (e) {
                  console.log('Message parse error:', e);
                }
              }
            });
          }
          
          // Vimeo Player API mesajlarÄ±nÄ± yakalamak iÃ§in
          if (window.addEventListener) {
            window.addEventListener('message', function(event) {
              if (event.origin === 'https://player.vimeo.com') {
                console.log('Vimeo message:', event.data);
                
                // Vimeo mesajlarÄ±nÄ± parse et
                if (event.data && typeof event.data === 'string') {
                  try {
                    const data = JSON.parse(event.data);
                    
                    // Ã–nemli event'leri handle et
                    switch (data.event) {
                      case 'ready':
                        console.log('Vimeo player ready');
                        isPlayerReady = true;
                        window.ReactNativeWebView.postMessage(JSON.stringify({event: 'ready'}));
                        break;
                      case 'play':
                        console.log('Vimeo play event');
                        window.ReactNativeWebView.postMessage(JSON.stringify({event: 'play'}));
                        break;
                      case 'pause':
                        console.log('Vimeo pause event');
                        window.ReactNativeWebView.postMessage(JSON.stringify({event: 'pause'}));
                        break;
                      case 'ended':
                        console.log('Vimeo ended event');
                        window.ReactNativeWebView.postMessage(JSON.stringify({event: 'ended'}));
                        break;
                      case 'timeupdate':
                        // Zaman gÃ¼ncellemelerini React Native'e gÃ¶nder
                        if (data.data && data.data.seconds && data.data.duration) {
                          window.ReactNativeWebView.postMessage(JSON.stringify({
                            event: 'timeupdate',
                            seconds: data.data.seconds,
                            duration: data.data.duration
                          }));
                        }
                        break;
                      default:
                        // DiÄŸer tÃ¼m mesajlarÄ± da gÃ¶nder
                        window.ReactNativeWebView.postMessage(JSON.stringify(data));
                        break;
                    }
                  } catch (e) {
                    // String data ise direkt gÃ¶nder
                    console.log('Raw Vimeo message:', event.data);
                    window.ReactNativeWebView.postMessage(JSON.stringify({
                      event: 'raw_message',
                      data: event.data
                    }));
                  }
                } else {
                  // Object data ise direkt gÃ¶nder
                  window.ReactNativeWebView.postMessage(JSON.stringify(event.data));
                }
              }
            });
          }
          
          // Page load durumunu kontrol et
          if (document.readyState === 'complete') {
            console.log('Document ready for background audio');
            window.ReactNativeWebView.postMessage(JSON.stringify({event: 'page_ready'}));
          } else {
            window.addEventListener('load', function() {
              console.log('Window loaded with background audio support');
              window.ReactNativeWebView.postMessage(JSON.stringify({event: 'page_loaded'}));
            });
          }
          
          true;
        `}
      />

